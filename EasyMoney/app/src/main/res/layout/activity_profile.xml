<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context=".Profile"
    android:background="#9E9E9E">

    <Button
        android:id="@+id/userProfileFoot"
        android:layout_width="108dp"
        android:layout_height="64dp"
        android:backgroundTint="#FFFFFF"
        app:icon="@drawable/ic_userinactive"
        app:iconGravity="top"
        app:iconTint="#676767"
        tools:layout_editor_absoluteX="306dp"
        tools:layout_editor_absoluteY="594dp" />

    <Button
        android:id="@+id/bEProfileFoot"
        android:layout_width="102dp"
        android:layout_height="64dp"
        android:backgroundTint="#FFFFFF"
        app:icon="@drawable/ic_bigexpenseinactive"
        app:iconGravity="top"
        app:iconTint="#FFFFFF"
        tools:iconTint="#676767"
        tools:layout_editor_absoluteX="204dp"
        tools:layout_editor_absoluteY="594dp" />

    <Button
        android:id="@+id/reportProfileFoot"
        android:layout_width="102dp"
        android:layout_height="64dp"
        android:backgroundTint="#FFFFFF"
        android:gravity="center"
        app:icon="@drawable/ic_reportincactive"
        app:iconGravity="top"
        app:iconTint="#676767"
        tools:layout_editor_absoluteX="102dp"
        tools:layout_editor_absoluteY="594dp" />

    <Button
        android:id="@+id/trackerProfileFoot"
        android:layout_width="102dp"
        android:layout_height="64dp"
        android:backgroundTint="#FFFFFF"
        app:icon="@drawable/ic_trackerinactive"
        app:iconGravity="top"
        app:iconTint="#676767"
        tools:layout_editor_absoluteY="594dp" />

    <TextView
        android:id="@+id/profileHeader"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="@string/profileHeader"
        android:textSize="30sp"
        tools:layout_editor_absoluteX="16dp"
        tools:layout_editor_absoluteY="22dp" />

    <Button
        android:id="@+id/helpProfileBtn"
        android:layout_width="52dp"
        android:layout_height="44dp"
        android:backgroundTint="#00FFFFFF"
        app:icon="@drawable/ic_help"
        app:iconTint="#676767"
        tools:iconGravity="top"
        tools:layout_editor_absoluteX="343dp"
        tools:layout_editor_absoluteY="19dp" />

    <TextView
        android:id="@+id/profileNametxt"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="Sohee Kim"
        tools:layout_editor_absoluteX="16dp"
        tools:layout_editor_absoluteY="79dp" />

    <TextView
        android:id="@+id/profileEmailtxt"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="bernardoasc@gmail.com"
        tools:layout_editor_absoluteX="16dp"
        tools:layout_editor_absoluteY="108dp" />

    <Button
        android:id="@+id/editProfileBtn"
        android:layout_width="92dp"
        android:layout_height="36dp"
        android:background="#00FFFFFF"
        android:outlineProvider="bounds"
        android:text="@string/editProfileBtn"
        android:textColor="#676767"
        android:textSize="8sp"
        tools:layout_editor_absoluteX="16dp"
        tools:layout_editor_absoluteY="127dp" />

    <androidx.constraintlayout.widget.ConstraintLayout
        android:id="@+id/cardView"
        android:layout_width="382dp"
        android:layout_height="155dp"
        android:background="#FFFFFF"
        tools:layout_editor_absoluteX="16dp"
        tools:layout_editor_absoluteY="180dp">

        <TextView
            android:id="@+id/profileFinanceInfotxt"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:text="Financial Information"
            android:textColor="#000000"
            tools:layout_editor_absoluteX="16dp"
            tools:layout_editor_absoluteY="16dp" />

        <Button
            android:id="@+id/profileEditIncomebtn"
            android:layout_width="38dp"
            android:layout_height="34dp"
            android:background="#00FFFFFF"
            app:icon="@drawable/ic_iconedit"
            app:iconGravity="top"
            app:iconTint="#676767"
            tools:layout_editor_absoluteX="302dp"
            tools:layout_editor_absoluteY="49dp" />

        <Button
            android:id="@+id/profileEditSavingbtn"
            android:layout_width="38dp"
            android:layout_height="34dp"
            android:background="#00FFFFFF"
            app:icon="@drawable/ic_iconedit"
            app:iconGravity="top"
            app:iconTint="#676767"
            tools:layout_editor_absoluteX="302dp"
            tools:layout_editor_absoluteY="94dp" />

        <TextView
            android:id="@+id/profileSavingOutputtxt"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:text="$4,500"
            android:textColor="#000000"
            tools:layout_editor_absoluteX="259dp"
            tools:layout_editor_absoluteY="102dp" />

        <TextView
            android:id="@+id/profileMonthOutputtxt"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:text="$4,500"
            android:textColor="#000000"
            tools:layout_editor_absoluteX="259dp"
            tools:layout_editor_absoluteY="57dp" />

        <TextView
            android:id="@+id/profileMonthIncometxt"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:text="Monthly Income"
            tools:layout_editor_absoluteX="26dp"
            tools:layout_editor_absoluteY="57dp" />

        <TextView
            android:id="@+id/profileSavingtxt"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:text="Current Savings"
            tools:layout_editor_absoluteX="26dp"
            tools:layout_editor_absoluteY="103dp" />

    </androidx.constraintlayout.widget.ConstraintLayout>

</androidx.constraintlayout.widget.ConstraintLayout>